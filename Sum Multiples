class Solution 
{
    public int sumOfMultiples(int n)
    {
        int sum = 0;
        for(int currentNum=1; currentNum <= n; currentNum++)
        {
            if(currentNum % 3 == 0 ||
                currentNum % 5 == 0 ||
                currentNum % 7 == 0){
                    sum += currentNum;
                }
        }

        return sum;        
    }
}
