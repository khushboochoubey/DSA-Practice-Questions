import java.util.*;

class dsa {

    static int sumOfDigits(int n) {
       if(n == 0)
         return 0;
       else
         return sumOfDigits(n/10) + n%10;

    }

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        System.out.println(sumOfDigits(n));
    }
}
