import java.util.*;

class dsa { 

    static int getLargest(int arr[]) {
     int res = 0;
     for (int i = 1; i < arr.length; i++) 
      if(arr[i]>arr[res]) 
          res = i; 
      return res;

    }

    public static void main(String[] args) {
      int arr[] = {5,2,9,3,9,10,35,37,11};
      System.out.println(getLargest(arr));
    }

    
}
