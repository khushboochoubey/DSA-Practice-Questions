class Solution {
    int print2largest(int arr[], int n) {
     // int res = -1 ;
      int largest = -1;
      int secondlargest = -1;
      for(int i = 0 ; i < arr.length ; i++) {
          if(arr[i] > largest){
            secondlargest = largest;
            largest =arr[i] ;
          }
      
      else if(  arr[i] <largest && arr[i]>secondlargest ){
        
        
          secondlargest = arr[i];
         
      }
  }
  return secondlargest;
  }
}
