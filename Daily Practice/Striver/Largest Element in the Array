import java.util.* ;
import java.io.*; 

public class Solution {

    static int largestElement(int[] arr, int n) {
        // Write your code here.
        int res = 0;
        for(int i = 0 ; i<n ; i++){
            if(arr[i] > res) 
               res=arr[i];
        }
        return res;

    }
}
