class Solution {
    public boolean arrayStringsAreEqual(String[] word1, String[] word2) {
        String wordString1 = "";
        String wordString2 = "";
        for(int i = 0 ; i < word1.length ; i++){
            wordString1 += word1[i];
        }

        for(int i = 0 ; i < word2.length ; i++){
            wordString2 += word2[i];
        }
        
        if( wordString1.equals(wordString2) ){
            return true;
        }

        return false;
    }
}
