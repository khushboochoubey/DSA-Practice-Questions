public class Solution {
    public static int team(int []skill, int n){
        // Write your code here.
        int count = 0;
        for(int i = 0 ; i < n ; i++){
            for(int j = i+1 ; j< n ; j++){
                 if(skill[i] > skill[j]*2)
                 count++ ;
            }
        }
        return count;
    }
}
