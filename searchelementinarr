import java.util.*;

class dsa {

    static int searchelementinarr(int arr[], int n, int x) {
        for (int i = 0; i < n; i++)
            if (arr[i] == x)
                return i;
        return -1;

    }

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int x = scanner.nextInt();
        int arr[] = { 1, 5, 7, 3, 7 };
        int n = arr.length;
        int r=searchelementinarr(arr, n, x);
        System.out.println(r);
    }
}
